	<queryString>
		<![CDATA[SELECT 
	tabelbarang.id,
	tabelbarang.kode,
	tabelbarang.nama,
	tblbarang_masuk.tgl,
	tblbarang_masuk_detail.jumlah,
	tabelbarang.satuan,
	tabelbarang.stok,
	tabelkategori.nama as nama_kategori,
	tabelsupplier.nama as nama_supplier
FROM tblbarang_masuk_detail
JOIN tblbarang_masuk ON tblbarang_masuk.id = tblbarang_masuk_detail.id_barang_masuk
JOIN tabelsupplier ON tabelsupplier.id = tblbarang_masuk.id_supplier
JOIN tabelbarang ON tabelbarang.id = tblbarang_masuk_detail.id_barang
JOIN tabelkategori ON tabelkategori.id = tabelbarang.id_kategori
WHERE tblbarang_masuk.tgl BETWEEN $P{d_dari} AND $P{d_sampai}]]>
	</queryString>
